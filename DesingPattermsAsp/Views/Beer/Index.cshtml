@model IEnumerable<DesingPattermsAsp.Models.ViewModels.BeerViewModel>
<div class="row">
    <div class="col-12">
        <a class="btn btn-primary" href="~/Beer/Add">Nueva Cerveza</a>
    </div>
</div>
<div class="row">
    <div class="col-12">
        <table class="table">
            <tr>
                <th>Nombre</th>
            </tr>
        </table>
        @foreach (var beer in Model)
        {
            <tr>@beer.Name</tr>
        }
    </div>
</div>